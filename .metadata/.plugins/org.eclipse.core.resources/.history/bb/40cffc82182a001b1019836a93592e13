package ch04;

class Car {
	public Car() {
		// TODO Auto-generated constructor stub
		System.out.println("Car");
	}
	
}

class Sonata extends Car{
	public Sonata() {
		super();
		// TODO Auto-generated constructor stub
		System.out.println("Sonata");
	}
}

public class Super01 {

	public static void main(String[] args) {
		new Sonata();

	}

}
