package review;


// 자바는 함수만 만들수가 없다.
// 더하기 함수를 만들고 싶다 => 더하기 함수를 만들려면 클래스가 필요함.

// 상태가 없는 메서드는 개념적으로 잘 못 됐다.

class Storage{
	String data;
	
	String download() {
		data = "GTA5";
		
		return data;
	}
}

public class Ex122 {

	public static void main(String[] args) {
		Storage s = new Storage();
		s.download();

	}

}
