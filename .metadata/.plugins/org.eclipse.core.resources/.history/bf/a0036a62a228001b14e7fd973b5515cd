package star1;

class Zealot implements StarUnit{
	String name;
	int hp;
	int attack;
	
//	규칙 : 클래스를 생성하면 무조건 초기화를 위한 생성자를 만든다.
	public Zealot(String name, int hp, int attack) {
	super();
	this.name = name;
	this.hp = hp;
	this.attack = attack;
}
	
	public Zealot() {
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public void attack() {
		
	}
	


	

}
