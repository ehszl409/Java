package ch05;

// 다형성을 성립하는 상속.
class 지구인{
	String name = "지구인";
}

class 한국인 extends 지구인{
	String name = "한국인";
}

class 부산인 extends 한국인{
	String name = "부산인";
}

public class Inherit02 {

	public static void main(String[] args) {
		// Heap 공간에 3개가 떳다.
		지구인 b = new 부산인();
		System.out.println(b.name);
		

	}

}
