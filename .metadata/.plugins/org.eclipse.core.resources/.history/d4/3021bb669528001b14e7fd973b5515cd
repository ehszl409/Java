package star1;

class Zealot implements StarUnit{
	String name;
	int hp;
	int attack;
	
//	규칙 : 클래스를 생성하면 무조건 초기화를 위한 생성자를 만든다.
	public Zealot(String n, int h, int a) {
		this.name = n;
		hp = h;
		attack = a;
		
	}
	
	@Override
	public void attack() {
		
		
	}

	public void status(String n, int h, int a) {
		this.name = n;
		this.hp = h;
		this.attack = a;
	}

}
