package ex01;

import java.util.Scanner;

public class MiddleValue {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int Value[] = {};
		for (int i = 0; i < 3; i++) {
			Value[i] = sc.nextInt();
		}
		
		int Max = 0;
		int MiddleValue = 0;
		
		if(Value [0] > Value[1] && Value[0] > Value[2]) {
			Max = Value[0];
			if (Value[1] > Value[2]) {
				MiddleValue = Value[1];
				System.out.println("중간값은 : " + MiddleValue);
			}
			else {
				MiddleValue = Value[2];
				System.out.println("중간값은 : " + MiddleValue);
			}
		}

	}

}
