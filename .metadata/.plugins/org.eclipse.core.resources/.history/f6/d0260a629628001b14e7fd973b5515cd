package star1;

class Dragoon implements StarUnit {
	String name;
	int hp;
	int attack;

	public Dragoon() {
		// TODO Auto-generated constructor stub
	}

	public Dragoon(String name, int hp, int attack) {
		this.name = name;
		this.hp = hp;
		this.attack = attack;
	}
	
	@Override
	public void attack() {
		// TODO Auto-generated method stub
		
	}
	
	public void status(String n, int h, int a) {
		n = this.name;
		h = this.hp;
		a = this.attack;
	}

}
