package ch05;

class Animal{
	
}

class Dog{
	void sound() {
		System.out.println("멍멍");
	}
}

class Cat{
	void sound() {
		System.out.println("야옹");
	}
}

public class Inherit03 {
	static void start(Dog d) {
		d.sound();
	}
	
	static void start(Cat d) {
		d.sound();
	}
	
	public static void main(String[] args) {
		Dog d = new Dog();
		Inherit03.start(d);
	}

}
